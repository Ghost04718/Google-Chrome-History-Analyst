{
  "manifest_version": 3,
  "name": "Google Chrome History Analyst",
  "version": "1.0",
  "description": "Review your yearly, monthly or weekly browsing history with AI insights.",
  "permissions": [
    "history",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "assets/icon16.png",
    "128": "assets/icon128.png"
  }
}